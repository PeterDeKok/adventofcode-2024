[*]
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[*.go]
indent_style = tab
tab_width = 4

# Disable a newline ending every file for lox (test) files
[*.txt]
indent_style = unset
indent_size = unset
tab_width = unset
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = false
insert_final_newline = false
max_line_length = off
